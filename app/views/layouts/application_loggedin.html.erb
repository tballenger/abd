<!DOCTYPE html>
<html>
<head>
  <title>Abohd</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<%= render 'layouts/header' %>

<div id="loggedin-wrapper">
<div class="container">
<!--menu partial-->
<div class="col-md-3 list-group">
	<%= link_to "Dashboard", dashboard_path, :class  => 'list-group-item' %>
	<%= link_to "Tenants", tenants_path, :class => 'list-group-item' %>
	<%= link_to "Messages", messages_path, :class => 'list-group-item' %>
	<%= link_to "Maintenance", maintenance_path, :class => 'list-group-item' %>
	<%= link_to "Financials", financials_path, :class => 'list-group-item' %>
	<%= link_to "Community", community_path, :class => 'list-group-item' %>
</div>
<!--end-->


<div class="col-md-9">
<%= yield %>
</div>

</div><!--container-->

</div>

<%= render 'layouts/footer' %>

</body>
</html>
